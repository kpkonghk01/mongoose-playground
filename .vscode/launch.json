{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "MemoryDb",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": ["memory"],
      "program": "${workspaceFolder}/index.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "DockerDb",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": ["mongo"],
      "program": "${workspaceFolder}/index.js"
    }
  ]
}